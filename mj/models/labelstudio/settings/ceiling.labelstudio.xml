            <Label value="0m" background="#FF01FF" hotkey="0"/>
            <Label value="1s" background="#00FF00" hotkey="q"/>
            <Label value="2s" background="#00FF1E" hotkey="w"/>
            <Label value="3s" background="#00FF3C" hotkey="e"/>
